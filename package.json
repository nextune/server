{
  "name": "nextune-server",
  "version": "0.3.0",
  "description": "backend server and aggregator for the nextune client",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist --copy-files --delete-dir-on-start && cd dist && sequelize db:migrate --env development",
    "start": "cd dist && node ./server.js",
    "dev": "npm run build && cd dist && nodemon ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextune/server.git"
  },
  "author": "rahul rao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nextune/server/issues"
  },
  "homepage": "https://github.com/nextune/server#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  }
}